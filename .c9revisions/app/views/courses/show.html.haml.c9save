{"ts":1364746215931,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"%p#notice= notice\n\n%p\n  %b Title:\n  = @course.title\n%p\n  %b Date:\n  = @course.date\n%p\n  %b Start time:\n  = @course.start_time\n%p\n  %b End time:\n  = @course.end_time\n%p\n  %b Venue:\n  = @course.venue\n%p\n  %b Description:\n  = @course.description\n%p\n  %b Price:\n  = @course.price\n\n\n\n%h2 Registered Participants\n%table\n  %tr\n    %th User\n    %th Feedback Form Completed?\n    %th Certificate Sent?\n    %th Paid?\n    %th status\n  -@course.registrations.each do |registration|\n    %tr\n      %td= registration.user.name\n      %td= registration.feedback_form_completed ? \"Yes\" : \"No\"\n      %td= registration.certificate_sent ? \"Yes\" : \"No\"\n      %td= registration.paid ? \"Yes\" : \"No\"\n      %td= registration.waiting_list ? \"Waiting List\" : \"Registered\"\n\n%h2 Add a Participant\n=form_for Registration.new do |f|\n  =f.hidden_field :course_id, value: @course.id\n  =f.label :name\n  =f.collection_select :user_id, User.order('name ASC'), :id, :name\n  =f.submit \"Add Participant\"\n\n\n= link_to 'Edit', edit_course_path(@course)\n\\|\n= link_to 'Back', courses_path"]],"start1":0,"start2":0,"length1":0,"length2":1042}]],"length":1042}
